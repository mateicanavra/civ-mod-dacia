  export NODE_PATH="/Users/mateicanavra/Documents/.nosync/DEV/civ-mod-dacia/node_modules/esbuild/bin/node_modules:/Users/mateicanavra/Documents/.nosync/DEV/civ-mod-dacia/node_modules/esbuild/node_modules:/Users/mateicanavra/Documents/.nosync/DEV/civ-mod-dacia/node_modules:/Users/mateicanavra/Documents/.nosync/DEV/node_modules:/Users/mateicanavra/Documents/.nosync/node_modules:/Users/mateicanavra/Documents/node_modules:/Users/mateicanavra/node_modules:/Users/node_modules:/node_modules:/Users/mateicanavra/Documents/.nosync/DEV/civ-mod-dacia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mateicanavra/Documents/.nosync/DEV/civ-mod-dacia/node_modules/esbuild/bin/node_modules:/Users/mateicanavra/Documents/.nosync/DEV/civ-mod-dacia/node_modules/esbuild/node_modules:/Users/mateicanavra/Documents/.nosync/DEV/civ-mod-dacia/node_modules:/Users/mateicanavra/Documents/.nosync/DEV/node_modules:/Users/mateicanavra/Documents/.nosync/node_modules:/Users/mateicanavra/Documents/node_modules:/Users/mateicanavra/node_modules:/Users/node_modules:/node_modules:/Users/mateicanavra/Documents/.nosync/DEV/civ-mod-dacia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
